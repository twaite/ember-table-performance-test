{{#each data as |test|}}
  {{test}}
{{/each}}

<EmberTable as |t|>
  <t.head @columns={{columns}} as |h|>
    <h.row as |r|>
      <r.cell as |column|>
        {{column.name}}
      </r.cell>
    </h.row>
  </t.head>

  <t.body @rows={{rows}} as |b|>
    <b.row as |r|>
      <r.cell as |value|>
        {{value}}
      </r.cell>
    </b.row>
  </t.body>
</EmberTable>